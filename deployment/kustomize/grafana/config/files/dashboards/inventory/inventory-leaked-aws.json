          "editorMode": "code",
          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "SELECT DISTINCT\n       i.region_name AS region,\n       COUNT(i.id) AS total,\n       gd.latitude AS lat,\n       gd.longitude AS lon,\n       gd.city AS city\nFROM aws_instance AS i\nINNER JOIN aws_geodata AS gd ON i.region_name = gd.region_name\nINNER JOIN g_machine AS m ON i.name = m.name\nINNER JOIN g_shoot AS s ON m.namespace = s.technical_id\nLEFT JOIN g_cloud_profile_aws_image AS cpaw ON s.cloud_profile = cpaw.cloud_profile_name AND i.image_id = cpaw.ami\nWHERE i.account_id IN ($aws_account) AND cpaw.ami IS NULL\nGROUP BY (i.region_name, gd.city, gd.latitude, gd.longitude)\n",
